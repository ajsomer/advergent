{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx src/db/migrate.ts",
    "migrate": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@clerk/backend": "^2.22.0",
    "@clerk/express": "^1.7.49",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.7",
    "esbuild": "0.25.12",
    "express": "^4.18.2",
    "google-ads-api": "^17.0.0",
    "googleapis": "^142.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "openai": "^6.9.0",
    "pg": "^8.11.3",
    "pino": "^8.16.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.2.3",
    "postgres": "^3.4.7",
    "svix": "^1.81.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.8",
    "drizzle-kit": "^0.31.7",
    "node-pg-migrate": "^6.2.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
